                            {{/if}}

                            <div class="class_buttons_div">
                                <div class="class_buttons">

                                    {{#if related_class_1_link}}
                                    <a class="button" href="{{related_class_1_link}}">{{related_class_1_name}}</a>
                                    {{/if}}

                                    {{#if related_class_2_link}}
                                    <a class="button" href="{{related_class_2_link}}">{{related_class_2_name}}</a>
                                    {{/if}}

                                    {{#if related_class_3_link}}
                                    <a class="button" href="{{related_class_3_link}}">{{related_class_3_name}}</a>
                                    {{/if}}
                                </div>
                            </div>
                        </div>
                        
                    </div>
                    
                    <div class="col-md-1"></div>

                
                    <div class="col-md-3 course_sidebar">
                        <div class="upcoming_course_div">
                            <h3>UPCOMING COURSE</h3>
                        </div>

                        <div class="upcoming_course_info">
                            <div style="padding-top: 10px">
                                <span class="glyphicon glyphicon-calendar"></span>
                                <h4>{{original_date}}</h4>
                            </div>

                            <div>
                                <span class="glyphicon glyphicon-hourglass"></span>
                                <h4>1-4PM (3 hours)</h4>
                            </div>

                            <div>
                                <span class="glyphicon glyphicon-user"></span>
                                <h4>Level: </h4><h4>&nbsp;{{level}}</h4>
                            </div>

                            <div style="padding-bottom: 30px">
                                <span class="glyphicon glyphicon-book"></span>
                                <h4>Prerequisite:&nbsp;</h4>
                                    {{#if prereq_1_name}}
                                        <h4><a href="{{prereq_1_link}}">{{prereq_1_name}}</h4><br>
                                        
                                            {{#if prereq_2_name}}
                                            <h4><a href="{{prereq_2_link}}">{{prereq_2_name}}</h4>
                                            {{/if}}
                                    {{/if}}

                                    {{#unless prereq_1_name}}
                                            <h4>None</h4>
                                    {{/unless}}
                            </div>
                        </div>
                        
                        {{#if register}}
                        <div class="register_button">
                            <h3><a href="http://{{register}}">REGISTER</a></h3>
                        </div>
                        {{/if}}
                        
                    </div>
                    
                </div>

            </div>
            <!-- section -->
        </div>
    {{/date}}